{
    "name": "The Things Network - Xively Connector",
    "description": "A app using Express 4",
    "repository": "https://github.com/embee8/ttn-xively-connector",
    "keywords": [
        "node",
        "ttn",
        "thethingsnetwork",
        "iot",
        "xively"
    ],
    "env": {
        "ttnAppEUI": {
            "description": "Your TTN appEUI",
            "required": false
        },
        "ttnAccessKey": {
            "description": "Your TTN access key",
            "required": false
        },
        "xivelyDeviceId": {
            "description": "The username / device ID of the paired Xively device",
            "required": false
        },
        "xivelyPassword": {
            "description": "The password of the paired Xively device",
            "required": false
        },
        "xivelyDeliveryTargetDeviceId": {
            "description": "The device to which messages are sent to",
            "required": false
        }
    },
    "buildpacks": [
        {
            "url": "https://elements.heroku.com/buildpacks/heroku/heroku-buildpack-nodejs"
        }
    ],
    "addons": [
        {
            "plan": "heroku-postgresql:hobby-dev",
            "as": "APP_DATABASE",
            "options": {
                "version": "9.5"
            }
        }
    ],
    "scripts": {}
}